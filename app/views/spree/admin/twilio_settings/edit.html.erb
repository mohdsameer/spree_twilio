<% content_for :page_title do %>
    <%= Spree.t(:order_sms_settings) %>
<% end %>
<% content_for :head do %>
    <%= javascript_include_tag 'spree/backend/utils.js' %>
    <%= javascript_include_tag 'spree/backend/intlTelInput.min.js' %>
    <%= stylesheet_link_tag 'spree/backend/intlTelInput' %>
<% end %>


<%= form_tag admin_twilio_setting_path, method: :put, id:"twilio-settings-form" do |f| %>
    <fieldset>
      <%= render 'form', f: f %>

      <div class="form-actions actions" data-hook="buttons"><%= button Spree.t('actions.update'), 'update' %></div>
    </fieldset>
<% end %>
